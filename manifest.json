{
"manifest_version": 2,
    "name": "WompMotions",
    "version": "1.0",
    "content_scripts": [
        {           
            "matches": ["*://chat.stackoverflow.com/*","*://chat.stackexchange.com/*","*://chat.meta.stackexchange.com/*"],
            "js": ["listofemotions.js","background.js"]
        }
    ],
    "browser_action": {
        "default_popup": "emotions.html",
        "default_icon": "icon.png"
    }
}